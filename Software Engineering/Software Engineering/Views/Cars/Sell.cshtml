@model Software_Engineering.Models.ViewModel
@{
    ViewBag.Title = "Sell";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">UnSold Cars</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <table id="example2" class="table table-bordered table-hover">
                    <thead>
                            <tr>
                                <th>
                                    Model
                                </th>
                                <th>
                                    Make
                                </th>
                                <th>
                                    Mileage
                                </th>
                                <th>
                                    Year
                                </th>
                                <th>
                                    CC
                                </th>
                                <th>
                                    Buying Price
                                </th>
                                <th>
                                    Selling Price
                                </th>
                                <th>
                                    Maintain Cost
                                </th>
                                <th>
                                    Condition
                                </th>
                                <th>
                                    Imported
                                </th>
                                <th>
                                    Owner
                                </th>
                                <th>
                                    Date Purchased
                                </th>
                                <th>
                                    Date Sold
                                </th>
                                <th>
                                    Reg No
                                </th>
                                <th>
                                    Customer
                                </th>
                                <th>
                                    Insurance
                                </th>
                                <th>
                                    Tracker
                                </th>
                                <th></th>
                            </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.UnSold)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Model)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Make)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Mileage)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Year)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.CC)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.buyingPrice)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.sellingPrice)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.maintainanceCost)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Condition)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Imported)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ownerName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.purchasedDate)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.soldDate)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.registerationNo)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Customer.Name)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Insurance.Company)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Tracker.Company)
                                </td>
                                <td>
                                    @Html.ActionLink("SellCar", "SellCar", new { id = item.Id })
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


